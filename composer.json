{
    "name": "mukulsmu/php-cfstream",
    "description": "Upload and manage Cloudflare Stream easily",
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.7|^6.0"
    },
    "authors": [
        {
            "name": "See Jian Jye",
            "email": "seejianjye@gmail.com"
        }
    ],
    "homepage": "https://github.com/mukulsmu/php-cfstream",
    "license": "MIT",
    "keywords": [
        "cfstream",
        "php-cfstream",
        "cloudflare stream",
        "jianjye",
        "tus"
    ],
    "autoload": {
        "psr-4": {
            "JianJye\\CFStream\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JianJye\\CFStream\\Laravel\\ServiceProvider"
            ]
        }
    }
